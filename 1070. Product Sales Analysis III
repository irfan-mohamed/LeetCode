-- Write your PostgreSQL query statement below
WITH select_first_year AS (
    SELECT
        product_id,
        year AS first_year,
        quantity,
        price,
        RANK() OVER(PARTITION BY product_id ORDER BY year ASC)
    FROM
        Sales
)
SELECT
    product_id,
    first_year,
    quantity,
    price
FROM 
    select_first_year
WHERE
    RANK = 1
;
