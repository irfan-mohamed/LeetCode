-- Write your PostgreSQL query statement below
WITH counted_cte AS (
    SELECT
        customer_id,
        COUNT(DISTINCT product_key) AS pc_count,
        (SELECT COUNT(*) FROM Product) AS pt_count
    FROM
        Customer
    GROUP BY
        customer_id
    ORDER BY
        customer_id
)
SELECT
    customer_id
FROM
    counted_cte
WHERE
    pc_count = pt_count
;
